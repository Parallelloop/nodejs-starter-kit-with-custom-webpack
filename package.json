{
  "name": "nodejs-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "stage:setup": "mup setup --config=./deploy/mup.staging.js",
    "stage:deploy": "npm run stage:build && mup deploy --config=./deploy/mup.staging.js",
    "stage:reconfig": "mup reconfig --config=./deploy/mup.staging.js",
    "stage:logs": "mup logs --config=./deploy/mup.staging.js --tail 500 -f",
    "stage:build": "rm -rf dist && webpack --config webpack.babel.js --progress",
    "stage:start": "node ./dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "axios": "^0.21.1",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.11.18",
    "morgan": "^1.10.0",
    "mup": "^1.5.3",
    "mup-node": "^0.8.0",
    "nodemailer": "^6.5.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
